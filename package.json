{
  "name": "parsegraph-weetcubes",
  "version": "1.5.0",
  "description": "Animated 3D cubes with eerie sound",
  "main": "dist/parsegraph-weetcubes.js",
  "scripts": {
    "coverage": "nyc npm run test",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha",
    "autotest": "cross-env TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha --watch",
    "demo": "node demo/demo.js",
    "autobuild": "webpack --watch --config webpack.config.js",
    "build": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/parsegraph/weetcubes.git"
  },
  "author": "Rainback, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parsegraph/weetcubes/issues"
  },
  "homepage": "https://github.com/parsegraph/weetcubes#readme",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "reporter": [
      "html"
    ],
    "exclude": [
      "test/**.js",
      "dist/**.js"
    ],
    "reportDir": "dist/coverage"
  },
  "dependencies": {
    "@atscm/esdoc-typescript-plugin": "^0.1.0",
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.6",
    "core-js": "^3.21.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "express": "^4.17.2",
    "glslify-loader": "^2.0.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "openssl": "^2.0.0",
    "prettier": "^2.5.1",
    "raw-loader": "^4.0.2",
    "source-map-loader": "^3.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.2.6",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.5.0",
    "ts-shader-loader": "^1.0.6",
    "typescript": "^4.5.5",
    "webgl-mock": "^0.1.7",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
